{
  "name": "Быстрый поиск в Яндекс/Google",
  "description": "Расширение для быстрого поиска в Яндексе или Google",
  "version": "0.2",
  "manifest_version": 2,

  "icons": {
    "16" : "icons/16.png",
    "64" : "icons/64.png",
    "128" : "icons/128.png"
  },

  "permissions": [
    "https://www.google.com/*",
    "https://yandex.ru/*"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}